#!/bin/bash

cmd="$1"
shift
args="$@"

pass=0

run_test(){
	num=$1
	expected=$2
	got=$("$cmd" "$args")
	if cmp -s <(echo $got) <(echo $expected); then
		echo "Test #$num: $(printf '\e[1;32mPASS\e[0m')"
		pass=$((pass + 1))
	else
		echo -e "Test #$num: $(printf '\e[1;31mFAIL\e[0m'). Expected:\n$expected\n  got:\n$got"
	fi
}

# Test 1
run_test 1 $'
.--------------------.
| TyIXL(             |
| x-IRr              |
| .----------------. |
| | .------------. | |
| | | cum Y      | | |
| | | Rj!wMTf y  | | |
| | | t .wtEKjlT | | |
| | \'------------\' | |
| | .------------. | |
| | | kOZ ATrn   | | |
| | | FY)BdGPXUL | | |
| | \'------------\' | |
| | .------------. | |
| | | WLq NYc\'f  | | |
| | | zCPW-WT    | | |
| | \'------------\' | |
| | .------------. | |
| | | zwgB.AdI   | | |
| | \'------------\' | |
| | .------------. | |
| | | cxFSoVEg   | | |
| | | ifHoC C    | | |
| | | RbbumPpNE  | | |
| | | cmUHzh)Cf  | | |
| | | cMaok      | | |
| | \'------------\' | |
| \'----------------\' |
| u", , s            |
\'--------------------\'
' <<EOD
